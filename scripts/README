# Preparing wikipedia

`
export WIKILANG='de'
./extract.sh

python scripts/chen2017/build_db.py "data/wiki/${WIKILANG}/*.bz2" "data/wiki/${WIKILANG}/${WIKILANG}wiki_chenprep.db" --preprocess scripts/chen2017/prep_wikipedia.py --num-workers 6

python scripts/chen2passagedb.py --lang $WIKILANG --chendb "data/wiki/${WIKILANG}/${WIKILANG}wiki_chenprep.db" --dst "data/wiki/${WIKILANG}" --strategy wrap
`
